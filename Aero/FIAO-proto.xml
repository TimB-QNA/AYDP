<?xml version="1.0"?>
<PropertyList>
 <generic>
  <input>
   <line_separator>newline</line_separator>
   <var_separator>tab</var_separator>
   <binary_mode>false</binary_mode>
   
   <chunk>
    <name>Elevator (+-1)</name>
    <type>double</type>
    <node>/controls/flight/elevator</node>
   </chunk>
   
   <chunk>
    <name>Aileron (+-1)</name>
    <type>double</type>
    <node>/controls/flight/aileron</node>
   </chunk>
   
   <chunk>
    <name>Throttle (0-1)</name>
    <type>double</type>
    <node>/controls/engines/engine/throttle</node>
   </chunk>

   <chunk>
    <name>Rudder (+-1)</name>
    <type>double</type>
    <node>/controls/flight/rudder</node>
   </chunk>

   <chunk>
    <name>Flaps (+-1)</name>
    <type>double</type>
    <node>/controls/flight/flaps</node>
   </chunk>
   
  </input>
 </generic>
</PropertyList>
